<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/j2ee/dtds/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>admin</servlet-name>
    <servlet-class>servlets.AdminServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>student</servlet-name>
    <servlet-class>servlets.StudentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>gradeExam</servlet-name>
    <servlet-class>servlets.ExamGradeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>examCreateServlet</servlet-name>
    <servlet-class>servlets.ExamCreateServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>examModifyServlet</servlet-name>
    <servlet-class>servlets.ExamModifyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>examTakeServlet</servlet-name>
    <servlet-class>servlets.ExamTakeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>examDeleteServlet</servlet-name>
    <servlet-class>servlets.ExamDeleteServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>examDisplayServlet</servlet-name>
    <servlet-class>servlets.ExamDisplayServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>admin</servlet-name>
    <url-pattern>/admin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>student</servlet-name>
    <url-pattern>/student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>gradeExam</servlet-name>
    <url-pattern>/gradeExam</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>examCreateServlet</servlet-name>
    <url-pattern>/createExam</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>examModifyServlet</servlet-name>
    <url-pattern>/modifyExam</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>examTakeServlet</servlet-name>
    <url-pattern>/takeExam</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>examDeleteServlet</servlet-name>
    <url-pattern>/deleteExam</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>examDisplayServlet</servlet-name>
    <url-pattern>/displayExam</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
</web-app>
